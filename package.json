{
  "name": "kenyanauto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "node ./postbuild.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "source": "dist",
    "include": [
      "/",
      "/About",
      "/Makes",
      "/Toyota",
      "/Toyota/prado",
      "/Toyota/prado-1st-gen",
      "/Toyota/prado-2nd-gen",
      "/Toyota/prado-third-gen",
      "/Toyota/prado-fourth-gen",
      "/Toyota/prado-fifth-gen",
      "/Toyota/rav4",
      "/Toyota/rav4-1st-gen",
      "/Toyota/rav4-2nd-gen",
      "/Toyota/rav4-3rd-gen",
      "/Toyota/rav4-4th-gen",
      "/Toyota/rav4-5th-gen",
      "/Toyota/harrier",
      "/Toyota/hilux",
      "/Toyota/vitz",
      "/Toyota/premio",
      "/Toyota/land-cruiser",
      "/Toyota/fielder",
      "/Toyota/chr",
      "/Toyota/fortuner",
      "/Toyota/HiAce",
      "/Mazda",
      "/Nissan",
      "/Honda",
      "/Audi",
      "/Subaru",
      "/Suzuki",
      "/car-details/:id/:nameSlug",
      "/Buying",
      "/guides/:route",
      "/model/:id"
    ],
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "crawl": true,
    "puppeteerIgnoreHTTPSErrors": true,
    "removeBlobs": true,
    "cacheAjaxRequests": true,
    "debug": true
  },
  "dependencies": {
    "@vercel/analytics": "^1.2.2",
    "axios": "^1.6.7",
    "ldrs": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-image-gallery": "^1.3.0",
    "react-router-dom": "^6.22.2",
    "react-select": "^5.8.0",
    "react-slick": "^0.30.2",
    "react-snap": "^1.23.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.1.4"
  }
}
